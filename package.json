{
  "name": "takabase-sharp",
  "version": "1.0.0",
  "description": "Node backend project",
  "author": "Denis Iakimenko",
  "private": true,
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "engines": {
    "node": "20"
  },
  "watch": {
    "start:functions": "*.ts"
  },
  "scripts": {
    "start:dev": "npm-watch start:functions",
    "start:deploy": "node firebase-deploy.js",
    "start:functions": "npm run build:prod && npx @google-cloud/functions-framework --target=sharp",
    "build:prod": "tsc -p tsconfig.json"
  },
  "keywords": [
    "node",
    "fastify",
    "sharp",
    "swagger",
    "javascript"
  ],
  "dependencies": {
    "@fastify/compress": "^6.2.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^2.1.0",
    "axios": "^1.6.7",
    "busboy": "^1.6.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.7.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/busboy": "^1.5.3",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fastify-tsconfig": "^2.0.0",
    "firebase-functions-test": "^3.1.1",
    "npm-check-updates": "^16.14.5",
    "npm-watch": "^0.11.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^3.0.3",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
